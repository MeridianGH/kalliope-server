{
  "name": "kalliopeserver",
  "version": "2.3.0",
  "main": "main.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsc -b --verbose && concurrently --restart-tries -1 --restart-after 5000 -n \"webpack,node\" -c \"bgBlue.bold,bgGreen.bold\" \"webpack --mode development --watch\" \"node . development\"",
    "build": "tsc && webpack",
    "start": "node .",
    "serve": "webpack serve --config webpack.config.js --mode development",
    "lint": "eslint .",
    "docs": "typedoc --tsconfig ./src/tsconfig.json",
    "prepare": "husky",
    "release": "commit-and-tag-version --releaseCommitMessageFormat \"chore: release {{currentTag}}\" --tag-prefix \"\""
  },
  "dependencies": {
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@phosphor-icons/react": "^2.1.7",
    "discord-api-types": "^0.37.23",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy": "^1.18.1",
    "imagescript": "^1.3.0",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-toastify": "^10.0.5",
    "typescript-eslint": "^8.8.1",
    "vanilla-tilt": "^1.8.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "1.0.1",
    "@eslint/js": "^9.0.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "commit-and-tag-version": "^12.4.1",
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "globals": "^15.3.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "sass": "^1.69.5",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.26.8",
    "typedoc-theme-hierarchy": "^5.0.3",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
