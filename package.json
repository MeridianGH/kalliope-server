{
  "name": "kalliopeserver",
  "version": "2.3.1",
  "main": "./server/main.js",
  "type": "module",
  "engines": {
    "node": "^20 || >= 22"
  },
  "private": true,
  "scripts": {
    "dev": "tsc -b --verbose && concurrently --restart-tries -1 --restart-after 5000 -n \"webpack,node\" -c \"bgBlue.bold,bgGreen.bold\" \"webpack --mode development --watch\" \"node . development\"",
    "build": "tsc && webpack",
    "start": "node .",
    "serve": "webpack serve --config webpack.config.js --mode development",
    "lint": "eslint .",
    "docs": "typedoc --tsconfig ./src/tsconfig.json",
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "preview-release": "npm run release -- --dry-run"
  },
  "dependencies": {
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@icons-pack/react-simple-icons": "^10.1.0",
    "@phosphor-icons/react": "^2.1.7",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "discord-api-types": "^0.37.101",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-proxy": "^1.18.1",
    "imagescript": "^1.3.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.26.2",
    "react-swipeable": "^7.0.1",
    "react-toastify": "^10.0.5",
    "vanilla-tilt": "^1.8.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/compat": "1.2.0",
    "@eslint/js": "^9.12.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "commit-and-tag-version": "^12.4.4",
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc-d5bba18b-20241009",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.11.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.6",
    "mini-css-extract-plugin": "^2.9.2",
    "rimraf": "^6.0.1",
    "sass": "^1.79.4",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.26.8",
    "typedoc-theme-hierarchy": "^5.0.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
