{
  "name": "kalliopeserver",
  "version": "2.4.1",
  "main": "./server/main.js",
  "type": "module",
  "engines": {
    "node": "^20 || >= 22"
  },
  "private": true,
  "scripts": {
    "dev": "tsc && concurrently --restart-tries -1 --restart-after 5000 -n \"webpack,node\" -c \"bgBlue.bold,bgGreen.bold\" \"webpack --mode development --watch\" \"node . development\"",
    "build": "tsc && webpack",
    "start": "node .",
    "serve": "tsc && concurrently --restart-tries -1 --restart-after 5000 -n \"webpack,nodemon,tsc    \" -c \"bgBlue.bold,bgGreen.bold,bgRed.bold\" \"webpack serve --mode development\" \"nodemon --watch ./server . development\" \"tsc --watch --preserveWatchOutput\"",
    "lint": "eslint .",
    "docs": "typedoc --tsconfig ./src/tsconfig.json",
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "preview-release": "npm run release -- --dry-run"
  },
  "dependencies": {
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@icons-pack/react-simple-icons": "^11.2.0",
    "@phosphor-icons/react": "^2.1.7",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "discord-api-types": "^0.37.119",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "http-proxy": "^1.18.1",
    "imagescript": "^1.3.0",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.1.5",
    "react-swipeable": "^7.0.2",
    "react-toastify": "^11.0.3",
    "vanilla-tilt": "^1.8.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@eslint/compat": "1.2.6",
    "@eslint/js": "^9.20.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "commit-and-tag-version": "^12.5.0",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.20.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typedoc": "^0.27.7",
    "typedoc-theme-hierarchy": "^5.0.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
